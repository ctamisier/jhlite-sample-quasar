{
  "name": "jhipster",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Project",
  "license": "UNLICENSED",
  "scripts": {
    "build": "cd src/main/webapp && npm run build",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "test": "cd src/main/webapp && npm run test"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.0.1",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.17"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
